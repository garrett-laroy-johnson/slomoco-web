---
layout: base.njk
---

<div class="row masonry" data-masonry='{"percentPosition": true }'>
  {% for ph in timetable %} {% for i in ph.img%}
  <div class="col-sm-4 my-2">
    <img src="/img/{{ i }}" class="img-fluid" alt="Responsive image" />
  </div>
  {% endfor %} {% for e in ph.events %}

  <div class="col-md-8 mb-2">
    <table class="table table-light table-striped table-hover">
      <tbody>
        {% for b in e.blocks %}
        <tr class="table-dark">
          <td colspan="3">
            <span ;> {{ e.date }} | {{ b.time }} UTC-5</span>
            {{ b.title }}
          </td>
        </tr>
        {% for p in b.presenters %}

        <tr>
          <td>
            <b>{{ p.name }} </b>
          </td>

          <td>
            <a href="/projects/{{ p.url }}">
              {{ p.proj }}
            </a>
          </td>

          <td>{{ p.type }}</td>
        </tr>

        {% endfor %} {% endfor %}
      </tbody>
    </table>
  </div>

  {% for i in e.img %}

  <div class="col-sm-4 my-2">
    <img src="/img/{{ i }}" class="img-fluid" alt="Responsive image" />
  </div>
  {% endfor %} {%endfor%} {%endfor%}
</div>
